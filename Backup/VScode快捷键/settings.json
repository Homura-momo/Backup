  "vim.smartRelativeLine": true,
  "vim.normalModeKeyBindingsNonRecursive": [
    {
      "before": [
        "H"
      ],
      "after": [
        "^"
      ]
    },
    {
      "before": [
        "L"
      ],
      "after": [
        "$"
      ]
    },
    {
      "before": [
        "g",
        "[",
      ],
      "commands": [
        "editor.action.marker.prevInFiles"
      ]
    },
    {
      "before": [
        "g",
        "]",
      ],
      "commands": [
        "editor.action.marker.nextInFiles"
      ]
    },
    {
      "before": [
        "g",
        "=",
      ],
      "commands": [
        "editor.action.formatDocument"
      ]
    },
    {
      "before": [
        "<",
      ],
      "after": [
        "<",
        "<",
      ]
    },
    {
      "before": [
        ">",
      ],
      "after": [
        ">",
        ">",
      ]
    },
    {
      "before": [
        "leader",
        "d"
      ],
      "after": [
        "\"",
        "_",
        "d",
      ]
    },
    {
      "before": [
        "leader",
        "c"
      ],
      "after": [
        "\"",
        "_",
        "c",
      ]
    },
    {
      "before": [
        "leader",
        "x"
      ],
      "after": [
        "\"",
        "_",
        "x",
      ]
    },
    /*     全局命令 */
    // 打开设置页面
    {
      "before": [
        "leader",
        "g",
        "s"
      ],
      "commands": [
        "workbench.action.openSettings"
      ]
    },
    // 打开热键映射
    {
      "before": [
        "leader",
        "g",
        "k"
      ],
      "commands": [
        "workbench.action.openGlobalKeybindings"
      ]
    },
    // 新建vscode实例
    {
      "before": [
        "leader",
        "g",
        "n"
      ],
      "commands": [
        "workbench.action.newWindow"
      ]
    },
    // 关闭vscode实例
    {
      "before": [
        "leader",
        "g",
        "q"
      ],
      "commands": [
        "workbench.action.closeWindow"
      ]
    },
    /*     文件 */
    // 新建文件
    {
      "before": [
        "leader",
        "f",
        "n"
      ],
      "commands": [
        "welcome.showNewFileEntries"
      ]
    },
    // 打开最近记录
    {
      "before": [
        "leader",
        "f",
        "h"
      ],
      "commands": [
        "workbench.action.openRecent"
      ]
    },
    // 关闭所有文件
    {
      "before": [
        "leader",
        "f",
        "Q"
      ],
      "commands": [
        "welcome.showNewFileEntries"
      ]
    },
    // 打开一个文件
    {
      "before": [
        "leader",
        "f",
        "f"
      ],
      "commands": [
        "workbench.action.files.openFile"
      ]
    },
    // 另存为文件
    {
      "before": [
        "leader",
        "f",
        "s"
      ],
      "commands": [
        "workbench.action.files.saveAs"
      ]
    },
    // 打开一个目录
    {
      "before": [
        "leader",
        "f",
        "d"
      ],
      "commands": [
        "workbench.action.files.openFolder"
      ]
    },
    //重新打开关闭的文件
    {
      "before": [
        "leader",
        "f",
        "r"
      ],
      "commands": [
        "workbench.action.reopenClosedEditor"
      ]
    },
    // 关闭文件
    {
      "before": [
        "leader",
        "f",
        "q"
      ],
      "commands": [
        "workbench.action.closeActiveEditors"
      ]
    },
    // 关闭所有文件
    {
      "before": [
        "leader",
        "f",
        "Q"
      ],
      "commands": [
        "workbench.action.closeAllEditors"
      ]
    },
    /*     导航 */
    // 切换侧边栏显示状态
    {
      "before": [
        "leader",
        "n",
        "n"
      ],
      "commands": [
        "workbench.action.toggleSidebarVisibility"
      ]
    },
    // 显示文件资源管理器
    {
      "before": [
        "leader",
        "n",
        "e"
      ],
      "commands": [
        "workbench.files.action.focusFilesExplorer"
      ]
    },
    // 显示TODO Tree
    {
      "before": [
        "leader",
        "n",
        "t"
      ],
      "commands": [
        "todo-tree-view.focus"
      ]
    },
    // 显示全局搜索
    {
      "before": [
        "leader",
        "n",
        "s"
      ],
      "commands": [
        "workbench.action.replaceInFiles"
      ]
    },
    //显示扩展
    {
      "before": [
        "leader",
        "n",
        "x"
      ],
      "commands": [
        "viewContainer.workbench.view.extensions.enabled"
      ]
    },
    //显示远程控制
    {
      "before": [
        "leader",
        "n",
        "r"
      ],
      "commands": [
        "workbench.view.scm"
      ]
    },
    //显示Git
    {
      "before": [
        "leader",
        "n",
        "g"
      ],
      "commands": [
        "workbench.view.scm"
      ]
    },
    /*     面板 */
    //显示面板
    {
      "before": [
        "leader",
        "p",
        "p"
      ],
      "commands": [
        "workbench.action.togglePanel"
      ]
    },
    // 显示命令
    {
      "before": [
        "leader",
        "p",
        "c"
      ],
      "commands": [
        "workbench.action.showCommands"
      ]
    },
    // 显示问题
    {
      "before": [
        "leader",
        "p",
        "m"
      ],
      "commands": [
        "workbench.actions.view.problems"
      ]
    },
    // 显示输出
    {
      "before": [
        "leader",
        "p",
        "o"
      ],
      "commands": [
        "workbench.action.output.toggleOutput"
      ]
    },
    // 显示终端
    {
      "before": [
        "leader",
        "p",
        "t"
      ],
      "commands": [
        "workbench.action.terminal.toggleTerminal"
      ]
    },
    // 显示调试控制台
    {
      "before": [
        "leader",
        "p",
        "d"
      ],
      "commands": [
        "workbench.debug.action.toggleRepl"
      ]
    },
  ],
